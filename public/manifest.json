{
  "name": "URL Shortener",
  "description": "URL Shortener Extension",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://gotiny.cc/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "logo16.png",
    "64": "logo64.png",
    "128": "logo128.png"
  },
  "manifest_version": 3,
  "version": "0.0.1",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://music.yandex.ru/;"
  }
}